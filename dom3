        <div id="a">
            <h1>Ravi</h1>
            <h1 id="rr">Ram</h1>
        </div>
        <div id="b">
            <h2>Sam</h2>
            <h2>venkat</h2>
        </div>
        <script>
        let n=document.createElement('h2')
        n.textContent='jaya'
        let p=document.getElementById("a")
        let l=document.getElementById("r")
        p.insertBefore(n,l)

        let nn=document.createElement('h2')
        nn.textContent='Ganesh'

        let pp=document.getElementById("a")
        let ll=document.getElementById("rr")
        pp.insertBefore(nn,ll)
        </script>
    </script>
</body></html>
